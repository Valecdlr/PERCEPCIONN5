function setup() {
  createCanvas(400,400);
}

function draw() {
  background(0,0,0);
  fill(250,0,0);
  rect(0,0,133,400);
  fill(0,255,0);
  rect(133,0,133,400);
  fill(0,0,100);
  rect(266,0,133,400);
}
